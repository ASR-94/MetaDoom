actor MetaSectorLava
{
	-SOLID
	+CANTLEAVEFLOORPIC
	+DONTSPLASH
	+NOTELEPORT
	MaxStepHeight 0
	speed 8
	radius 1
	height 1
	states
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 
		Idle:
			TNT1 A 8 A_Wander
			TNT1 A 0 A_Jump(16,"Parp")
			TNT1 A 0 A_Jump(128,"Croak")
			loop
		Parp:
			TNT1 A 0 A_SpawnItem("LavaSmoke")
			goto Croak
		Croak:
			LABL ABCDE 4
			goto Idle
	}
}