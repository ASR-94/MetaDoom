actor MetaSectorNukageCore
{
	-SOLID
	-NOCLIP
	+DONTSPLASH
	+NOTELEPORT
	radius 1
	height 1
	states
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0
			{
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
				A_SpawnItemEx("MetaSectorNukage",0,0,0,random(8,32),random(8,32),0,random(0,360));
			}
		Croak:
			TNT1 A 0 A_PlaySound("world/acid")
		Idle:
			TNT1 A 8
			TNT1 A 0 A_Jump(128,"Croak")
			loop
	}
}

actor MetaSectorNukage
{
	-SOLID
	+CANTLEAVEFLOORPIC
	+DONTSPLASH
	+NOTELEPORT
	MaxStepHeight 0
	speed 8
	radius 1
	height 1
	states
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 
		Idle:
			TNT1 A 8 A_Wander
			TNT1 A 0 A_Jump(128,"Croak")
			loop
		Croak:
			NUBL ABCDE 4
			goto Idle
	}
}

