// The Wretched is based on Doom 4's Caco, and has a version of the nasty tongue
// attack from the Hell Followed DLC. I don't much like it's sprites, just
// quietly, but they'll do for now.

ACTOR MetaWretched : MonsterCore
{
	Health 450
	GibHealth 50
	Radius 31
	Height 56
	Mass 400
	Speed 8
	PainChance 128
	Monster
	+FLOAT +NOGRAVITY +FLOATBOB
	SeeSound "monsters/wretched/see"
	PainSound "monsters/wretched/pain"
	DeathSound "monsters/wretched/die"
	ActiveSound "monsters/wretched/idle"
	Obituary "$OB_WRETCHED"
	HitObituary "$OB_WRETCHED"
	Bloodtype Blueblood
	BloodColor "0 0 80"
	Decal "bluebloodsplat"
	scale 0.65
	Species "Cacodemon"
	States
	{
	Spawn:
		WRET AB 10 A_Look
		Loop
	See:
		"####" A 0 A_MetaBleed("FlyingBlood")
		"####" A 0 
		{
			// if too close to the ground, punt the wretched up in the air
			// slightly to avoid FLOATBOB ugliness
			if (z <= (floorz + 16))
			{
				ThrustThingZ(0,2,0,0);
			}
		}
		WRET AACCBBDD 3 A_Chase
		Loop
	Scurred:
		WRET A 0 A_ChangeFlag("FRIGHTENED", TRUE)
	ScurredLoop:
		WRET A 0 A_GiveInventory("ScurredCounter", 1)
		WRET A 2 A_Chase("", "")
		WRET A 2 A_Chase("", "")
		WRET A 0 A_SpawnItem("HolySweat",0,72)
		WRET C 2 A_Chase("", "")
		WRET C 2 A_Chase("", "")
		WRET B 2 A_Chase("", "")
		WRET B 2 A_Chase("", "")
		WRET C 0 A_SpawnItem("HolySweat",0,72)
		WRET D 2 A_Chase("", "")
		WRET D 2 A_Chase("", "")
		WRET D 0 A_JumpIfInventory("ScurredCounter", 6, "ScurredEnd")
		loop
	ScurredEnd:
		WRET D 0
		{
			A_ChangeFlag("FRIGHTENED", FALSE);
			A_TakeInventory("ScurredCounter",999);
		}
		goto See
	Missile:
		WRET A 0 A_Jump(64, "MissileTongue")
		WRET E 5 
		WRET F 5 BRIGHT { A_FaceTarget; A_CustomMissile ("MetaCacodemonBall", 24, 0, 0); }
		WRET G 5 A_FaceTarget
		Goto See
	MissileTongue:
		WRET A 5 A_FaceTarget
		WRET H 5 A_FaceTarget
		WRET I 0 A_Stop
		WRET I 20 BRIGHT { A_FaceTarget; A_CustomMissile ("WretchedTongue", 24, 0, 0); }
		WRET JA 5 A_FaceTarget
		Goto See
	Melee:
		WRET A 4 A_FaceTarget
		WRET E 4 A_FaceTarget
		WRET I 4 BRIGHT A_FaceTarget
		WRET A 0 BRIGHT A_CustomMeleeAttack(10, "monsters/painelemental/bite", "monsters/painelemental/bite", PainElementalBite, 1)
		WRET A 4 A_FaceTarget
		WRET J 4 A_FaceTarget
		Goto See
	Pain:
		WRET K 3
		WRET K 6 A_Pain
		Goto See
	Pain.HolyWater:
		WRET K 3
		WRET K 6 A_Pain
		Goto Scurred
	Death:
		WRET L 0 A_GiveToTarget("ScoreItem", 500)
		WRET L 0 A_Jump (128, 2)
		WRET L 0 A_ChangeFlag("SPRITEFLIP", TRUE) // random chance of sprite flipping
		TNT1 A 0 A_SpawnItem("SoulSpawner")
		WRET L 6 A_ChangeFlag("FLOATBOB", FALSE)
		WRET M 6 A_Scream
		WRET N 6 
		//WRET O 6
		WRET OOOOOO 1 A_CheckSolidFooting("DeathLandSilent")
		WRET O 1 A_CheckSolidFooting("DeathLand")
		wait
	DeathLand:
		WRET P 0 A_PlaySound("bodyfalls/cacodemon")
	DeathLandSilent:
		WRET P 6 A_NoBlocking
		WRET Q -1 A_SetFloorClip
		Stop
	Death.Gauss:
	XDeath:
		TNT1 A 0 A_SpawnItem("SoulSpawner")
		WRET L 0 A_GiveToTarget("ScoreItem", 400)
		WRET L 0 A_ChangeFlag("FLOATBOB", FALSE)
		WRET L 3 A_Fall
		TNT1 A 0 A_PlaySound("monsters/caco/gib")
		TNT1 A 0
		{
			A_CustomMissile ("BlueFlyingBlood", random(8,32), 0, random (0, 360), 2, random (0, 160));
			A_CustomMissile ("BlueFlyingBlood", random(8,32), 0, random (0, 360), 2, random (0, 160));
			A_CustomMissile ("BlueFlyingBlood", random(8,32), 0, random (0, 360), 2, random (0, 160));
